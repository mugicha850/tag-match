<% @title = "職員の新規登録" %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @external_coach, url: :external_coach_users do |f| %>
    <%= render "form", f: f %>
    <div class="buttons">
      <%= f.submit "登録" %>
      <%= link_to "キャンセル", :external_coach_users %>
    </div>
  <% end %>
</div>