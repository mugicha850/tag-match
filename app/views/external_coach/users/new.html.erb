<% @title = "職員の新規登録" %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_with model: @external_coach, url: :external_coach_users do |form| %>
    <%= render "shared/errors", obj: @external_coach %>
    <%= render "shared/user/form", form: form %>
    <div class="buttons">
      <%= form.submit "登録" %>
      <%= link_to "キャンセル", :external_coach_users %>
    </div>
  <% end %>
</div>